@model yeniBingöl.Models.Ilceler

<h2>İlçe Düzenle</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
<div>
    <label>İlçe Adı:</label>
    @Html.TextBoxFor(model => model.IlceAd)
    @Html.ValidationMessageFor(model => model.IlceAd, "", new { @class = "text-danger" })

</div>
    <button type="submit" class="btn btn-primary">Güncelle</button>
}
